    </div>
  </div>
  <!-- Page Wrapper -->
  <div id="wrapper">
    <!-- Sidebar -->
    <ul class="navbar-nav bg-purple sidebar sidebar-dark accordion" id="accordionSidebar">
      <!-- Sidebar - Brand -->
      <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/admin/home">
        <div class="sidebar-brand-icon rotate-n-15">
          <i class="fas fa-house fa-2x"></i>
        </div>
        <div class="sidebar-brand-text mx-3">
          <b style="font-family: Georgia; color: gold">Mi</b>-<i>Assets</i>
        </div>
      </a>

      <!-- Divider -->
      <hr class="sidebar-divider my-1 bg-warning" />

      <!-- Nav Item - Dashboard -->
      <li class="nav-item  side-nav">
        <a class="nav-link" href="/admin/home">
          <i class="fas fa-fw fa-tachometer-alt"></i>
          <span>Dashboard</span></a>
      </li>

      <!-- Divider -->
      <hr class="sidebar-divider" />

      <!-- Heading -->
      <div class="sidebar-heading">Add</div>

      <!-- Nav Item - Pages Collapse Menu -->
      <li class="nav-item side-nav">
        <a class="nav-link collapsed" href="/admin/add-renter" aria-expanded="true" aria-controls="collapseTwo">
          <i class="fas fa-fw fa-user"></i>
          <span>Renters</span>
        </a>

      </li>

      <!-- Nav Item - fixed assets Collapse Menu -->
      <li class="nav-item side-nav">
        <a class="nav-link collapsed" href="/admin/add-asset" aria-expanded="true" aria-controls="collapseUtilities">
          <i class="fas fa-fw fa-wrench"></i>
          <span>Fixed assets</span>
        </a>

      </li>
      <li class="nav-item  side-nav">
        <a
          class="nav-link "
          href="/admin/add-admin"
          aria-expanded="true"
          aria-controls="collapseUtilities"
        >
          <i class="fas fa-fw fa-users"></i>
          <span>New Admin</span>
        </a>
      </li>

      <!-- Divider -->
      <hr class="sidebar-divider" />

      <!-- Heading -->
      <div class="sidebar-heading">information</div>

      <!-- Nav Item - Pages Collapse Menu -->
      <li class="nav-item side-nav">
        <a class="nav-link collapsed" href="/admin/renters" aria-expanded="true" aria-controls="collapsePages">
          <i class="fas fa-fw fa-users"></i>
          <span>Rentals Details</span>
        </a>

      </li>

      <!-- Nav Item - Charts -->
      <li class="nav-item side-nav">
        <a class="nav-link" href="/assets">
          <i class="fas fa-fw fa-folder"></i>
          <span>Assets Details</span></a>
      </li>

      <!-- Nav Item - Tables -->
      <li class="nav-item side-nav">
        <a class="nav-link" href="/admin/depreciate">
          <i class="fas fa-fw fa-egg"></i>
          <span>Depreciation</span></a>
      </li>
      <li class="nav-item active side-nav">
        <a class="nav-link " href="/admin/reminder">
          <i class="fas fa-fw fa-mail-bulk"></i>
          <span>Reminders</span></a>
      </li>
      <li class="nav-item  side-nav">
        <a class="nav-link" href="/admin/profit">
          <i class="fas fa-fw fa-kiwi-bird"></i>
          <span>Profit</span></a>
      </li>
      <li class="nav-item side-nav">
        <a class="nav-link" href="/admin/profile">
          <i class="fas fa-fw fa-circle-user"></i>
          <span>Profile</span></a>
      </li>
      <li class="nav-item side-nav">
        <a class="nav-link" href="" data-bs-toggle="modal" data-bs-target="#my_Modal">
          <i class="fas fa-fw fa-sign-out"></i>
          <span>Sign Out</span></a>
      </li>
      <!-- The Modal -->
      <div class="modal" id="my_Modal">
        <div class="modal-dialog">
          <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
              <h4 class="modal-title">Did you want to sign Out?</h4>
              <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>

            <!-- Modal body -->
            <div class="modal-body">
              Are you sure you want to long out?
            </div>

            <!-- Modal footer -->
            <div class="modal-footer">
              <a type="button" href class="btn btn-primary">Yes</a>
              <button type="button" class="btn btn-danger" data-bs-dismiss="modal">No</button>
            </div>

          </div>
        </div>
      </div>
      <!-- Divider -->
      <hr class="sidebar-divider d-none d-md-block" />



      <!-- Sidebar Message -->
      <div class="sidebar-card d-none d-lg-flex">
        <% if (admin.photo) { %>
          <img class="sidebar-card-illustration mb-2" src="<%= admin.photo %>" alt="" />
         
        <% } else{ %>
        <img class="sidebar-card-illustration mb-2" src="/img/profile.jpg" alt="" />
  <% } %>
        <p class="text-center mb-2">
          <strong title="my profile"> <a href="/admin/profile"><%- include("../patials/adminName")  %></a></strong> welcome to Mi assets,

        </p>
      </div>
    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
          <!-- Sidebar Toggle (Topbar) -->
          <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
            <i class="fa fa-bars"></i>
          </button>


          <!-- Topbar Navbar -->
          <%- include("../patials/adminHeader") %>

        </nav>
        <section class="section profile">
          <div class="row">


            <div class="col-xl-12 ">
              <div class="card">
                <div class="card-body pt-3">
                  <!-- Bordered Tabs -->
                  <ul class="nav nav-tabs nav-tabs-bordered ">

                    <li class="nav-item">
                      <button class="nav-link active" data-bs-toggle="tab" data-bs-target="#reminder">Construct Reminder</button>
                    </li>

                    <li class="nav-item">
                      <button class="nav-link" data-bs-toggle="tab" data-bs-target="#history">Histroy</button>
                    </li>


                  </ul>
                  <div class="tab-content pt-2">

                    <div class="tab-pane fade show active profile-overview" id="reminder">

                      <div class="card-body profile-card pt-2  flex-column align-items-center">

                        <!-- reminder -->
                        <form action="" class="mt-2" method="post">
                          <div class="row">
                            <div class="col-md-4 col-sm-4 col-lg-4">
                              <!-- full name -->
                              <div class="label ">Full Name</div>
                              <select class="form-select form-select-lg" name="fullname" id="">
                                <% for( let renter of renters ) { %>
                                  <option value="<%= renter.fullName() %>" ><%=renter.fullName() %></option>
                                  
                                  <% } %>
                                </select>
                              </div>

                              <!-- Email -->
                            <div class="col-md-4 col-sm-4 col-lg-4 mb-1">
                                <label for="" class="form-label">Email</label>

                                <select class="form-select form-select-lg" name="email" id="">
                              <% for( let renter of renters ) { %>
                                <% let arr = [] %>
                                <% let em =renter.email
                                // let k = eamil.push()
                                %>
                                <%  console.log(renter.email) %>
                                  <option value="<%= renter.email %>" ><%=renter.email %></option>
                                  <% } %>
                                </select>

                            </div>

                            <div class="col-md-4 col-sm-4 col-lg-4">
                              <!-- department -->
                              <div class="">
                                <label for="" class="form-label">Title</label>
                                <input type="text" class="form-control" name="title" id="" aria-describedby="helpId" placeholder="Title of the messaage">
                              </div>

                            </div>
                          </div>
                          <div class="row">
                            <div class="mb-3">
                              <label for="" class="form-label">Messages</label>
                              <textarea class="form-control" name="message" id="" rows="3"></textarea>
                            </div>
                          </div>
                          <div class="text-center">

                            <button type="submit" class="btn btn-primary text-center">Send Mail</button>
                          </div>
                        </form>

</div>
</div>
                      </div>
                    </div>
                  </div>

                  <div class="tab-pane fade profile-edit pt-3" id="history">

                    <div class="tab">

                      <p>
                        <a class="btn w-25" data-bs-toggle="collapse" href="#contentId" aria-expanded="false" aria-controls="contentId">
                          adewale adetunji <span class="float-right">12:00</span>
                        </a>
                      </p>
                      <div class="collapse" id="contentId">
                        you received a reminder message on 20th january 2023
                      </div>
                    </div>

                    <div class="tab">

                      <p>
                        <a class="btn w-25" data-bs-toggle="collapse" href="#content" aria-expanded="false" aria-controls="contentId">
                          adewale adetunji <span class="float-right">14:00</span>
                        </a>
                      </p>
                      <div class="collapse" id="content">
                        you received a reminder message on 20th january 2023
                      </div>
                    </div>

                    <div class="tab">

                      <p>
                        <a class="btn w-25" data-bs-toggle="collapse" href="#cont" aria-expanded="false" aria-controls="contentId">
                          adewale adetunji <span class="float-right">24:00</span>
                        </a>
                      </p>
                      <div class="collapse " id="cont">
                        you received a reminder message on 20th january 2023
                      </div>
                    </div>




                  </div>
                </div>

              </div>
            </div>
        </section>
        <!-- The Modal -->
        <div class="modal fade" id="myModal">
          <div class="modal-dialog modal-dialog-sm">
            <div class="modal-content">

              <!-- Modal Header -->
              <div class="modal-header">
                <h4 class="modal-title"></h4>
                <button type="button" class="close" data-bs-dismiss="modal">&times;</button>
              </div>

              <!-- Modal body -->
              <div class="modal-body">
                <h6>Are you sure you want to log out?</h6>
              </div>

              <!-- Modal footer -->
              <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal"> <a href="/admin">yes</a> </button>
                <button type="button" class="btn btn-danger" data-dismiss="modal"> <a href="#">NO</a> </button>
              </div>

            </div>
          </div>
          <!-- upload modal -->
          </main>
          <!-- End #main -->


          <!-- jquery link -->
          <script src="/js/jquery.min.js"></script>
          <script>
            $(window).on('load', function() {
              setTimeout(function() {
                $(".preloader").fadeOut('slow');
              }, 600);
            });
          </script>
